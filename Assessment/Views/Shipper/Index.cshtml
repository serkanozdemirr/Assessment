@model List<Assessment.Core.Models.Shipper>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h1>Shipper List</h1>
<br />
<table class="table table-dark table-striped table-hover">
    <tr>
        <th>ID</th>
        <th>Company Name</th>
        <th>Phone</th>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.Id</td>
            <td>@x.CompanyName</td>
            <td>@x.Phone</td>

        </tr>
    }
</table>